            },
            {
              "caption": "Gerätetyp",
              "name": "Type",
              "width": "200px",
              "add": 0,
              "edit": {
                "type": "Select",
                "options": [
                  {
                    "caption": "Bitte auswählen",
                    "value": 0
                  },
                  {
                    "caption": "1 - Variable",
                    "value": 1
                  },
                  {
                    "caption": "2 - Skript",
                    "value": 2
                  },
                  {
                    "caption": "3 - HmIP-BSL (Kanal 8 - LED oben)",
